{
  "name": "finbuddy",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "update:production": "eas update --branch production",
    "update:preview": "eas update --branch preview",
    "update:development": "eas update --branch development",
    "push:production": "eas update --branch production --message 'Grafik iyileştirmeleri ve temizlik'",
    "push:preview": "eas update --branch preview --message 'Grafik iyileştirmeleri ve temizlik'",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@sentry/react-native": "^7.2.0",
    "expo": "~54.0.7",
    "expo-dev-client": "^6.0.12",
    "expo-device": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-notifications": "~0.32.11",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.10",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-native": "^0.81.4",
    "react-native-onboarding-swiper": "^1.3.0",
    "react-native-safe-area-context": "~5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "~19.1.13",
    "@types/react-native": "^0.73.0",
    "jsdom": "^25.0.1",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.3"
  },
  "private": true
}
