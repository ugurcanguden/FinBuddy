{
  "expo": {
    "name": "FinBuddy",
    "slug": "FinBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#001f3f"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "windowSoftInputMode": "adjustResize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "guden"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0087995b-1520-4e81-aa5a-d2fe8f1c05d5"
      },
      "environment": "production"
    },
    "owner": "ugurcanguden",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0087995b-1520-4e81-aa5a-d2fe8f1c05d5"
    }
  }
}
