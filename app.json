{
  "expo": {
    "name": "FinBuddy",
    "slug": "finbuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#001f3f"
    },
    "notification": {
      "icon": "./assets/notifications/ic_stat_finbuddy_24.png",
      "color": "#001f3f",
      "androidMode": "default",
      "androidCollapsedTitle": "FinBuddy"
    },
    "ios": {
      "bundleIdentifier": "com.finbuddy.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.finbuddy.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "windowSoftInputMode": "adjustResize",
      "notification": {
        "icon": "./assets/notifications/ic_stat_finbuddy_24.png",
        "color": "#001f3f"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "guden"
        }
      ],
      "expo-dev-client"
    ],
    "extra": {
      "environment": "production",
      "eas": {
        "projectId": "42af4bfb-b4f0-40b1-ad1c-7560a23a1a89"
      }
    },
    "owner": "ugurcanguden",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0087995b-1520-4e81-aa5a-d2fe8f1c05d5"
    }
  }
}
